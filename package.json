{
  "name": "bestyjs",
  "packageManager": "yarn@3.1.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "fmt": "prettier --write packages",
    "fmt:check": "prettier --check packages",
    "frontend:deploy": "yarn set version 3.1.1 && yarn workspace frontend build"
  },
  "devDependencies": {
    "prettier": "^2.5.1"
  }
}
